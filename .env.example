# ==========================================
# 새 프로젝트 시 모든 값을 새로 발급받아야 함
# 복사: cp .env.example .env.local
# ==========================================

# --- Neon PostgreSQL ---
DATABASE_URL=

# --- Google OAuth (NextAuth) ---
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
NEXTAUTH_SECRET=        # ⚠️ 프로젝트마다 반드시 다르게! (같으면 로그인 세션 충돌)
NEXTAUTH_URL=           # 예: http://localhost:5005

# --- 사이트 URL ---
NEXT_PUBLIC_SITE_URL=   # 예: https://www.example.com

# --- Cloudflare R2 ---
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=
R2_ACCOUNT_ID=
R2_BUCKET_NAME=
R2_PUBLIC_URL=

# --- Google Analytics (선택) ---
GOOGLE_PRIVATE_KEY=
GOOGLE_SERVICE_ACCOUNT_EMAIL=
GA_PROPERTY_ID=
NEXT_PUBLIC_GA_MEASUREMENT_ID=
